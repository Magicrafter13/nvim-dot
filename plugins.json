	"_c03": "Standard plugins.",
	"lightline": {
		"repo": "itchyny/lightline.vim",
		"comment": "Fancy status bar at bottom of screen"
	},
	"feline": {
		"repo": "freddiehaddad/feline.nvim",
		"comment": "Minimal, customizable statusline."
	},
	"lualine": {
		"repo": "nvim-lualine/lualine.nvim",
		"comment": "Fast, easy to configure statusline."
	},
	"undotree": {
		"repo": "mbbill/undotree",
		"comment": "Visual Undo Tree"
	},
	"treesitter": {
		"repo": "nvim-treesitter/nvim-treesitter",
		"params": [
			"build = function() vim.cmd('TSUpdate') end"
		],
		"comment": "Code folding"
	},
	"suda": {
		"repo": "lambdalisue/suda.vim",
		"comment": "Alternative for suda.vim, adds :SudaWrite (Neovim specific, workaround for bug)"
	},
	"nvim-web-devicons": {
		"repo": "nvim-tree/nvim-web-devicons",
		"comment": "Nerd font icons for languages and such, with colors included. \"Fork\" of vim-devicons."
	},



	"_c04": "Git related plugins.",
	"vim-fugitive": {
		"repo": "tpope/vim-fugitive",
		"comment": "Allows running git commands from ViM, with :Git"
	},
	"vim-gitgutter": {
		"repo": "airblade/vim-gitgutter",
		"comment": "When looking at modified file in git repo, shows markings (+, -, ~) on side to indicate changes"
	},
	"gitsigns_nvim": {
		"repo": "lewis6991/gitsigns.nvim",
		"comment": "Super fast git decorations implemented purely in Lua."
	},



	"_c10": "General programming plugins.",
	"vim-rainbow": {
		"repo": "frazrepo/vim-rainbow",
		"comment": "Multi-color brackets for ease-of-use"
	},
	"auto-pairs": {
		"repo": "jiangmiao/auto-pairs",
		"comment": "Automatically creates the closing brackets when the first one is typed"
	},
	"vim-numbertoggle": {
		"repo": "jeffkreeftmeijer/vim-numbertoggle",
		"comment": "Toggle relativenumber based on current context"
	},



	"_c11": "LSP and linting.",
	"nvim-lspconfig": {
		"repo": "neovim/nvim-lspconfig",
		"comment": "Premade LSP configs for various languages"
	},
	"fidget": {
		"repo": "j-hui/fidget.nvim",
		"params": [
			"tag = 'legacy'",
			"lazy = true",
			"event = 'LspAttach'"
		],
		"comment": "nvim-lsp progress UI"
	},
	"ale": {
		"repo": "dense-analysis/ale",
		"comment": "Code linter"
	},
	"syntastic": {
		"repo": "scrooloose/syntastic",
		"comment": "Syntax checking plugin, that runs through external syntax checkers"
	},



	"_c12": "Nerdtree",
	"nerdtree": {
		"repo": "preservim/nerdtree",
		"params": [
			"lazy = true",
			"dependencies = {'nerdtree-git-plugin', 'nvim-web-devicons'}",
			"cmd = 'NERDTreeToggle'"
		],
		"comment": "File tree explorer. Open with :NERDTree"
	},
	"nerdtree-git-plugin": {
		"repo": "Xuyuanp/nerdtree-git-plugin",
		"params": [
			"lazy = true"
		],
		"comment": "Adds git markers to NERDTree (like showing changes, or ignored files)",
		"attributes": [ "nerdtree" ]
	},



